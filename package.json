{
  "name": "awesome-nest-boilerplate",
  "version": "0.0.1",
  "description": "",
  "author": "Jack <darryl.42730810@nicoric.com>",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build:prod": "tsc -p tsconfig.build.json",
    "type-check": "tsc --noEmit --skipLibCheck",
    "start:dev": "ts-node src/main.ts",
    "start:prod": "node dist/main.js",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm-extension": "ts-node -r tsconfig-paths/register ./node_modules/typeorm-extension/bin/cli.cjs",
    "migration:generate": "yarn run typeorm --dataSource=src/data-source.ts migration:generate",
    "migration:create": "yarn run typeorm migration:create",
    "migration:run": "yarn run typeorm --dataSource=src/data-source.ts migration:run",
    "new": "hygen new",
    "migration:revert": "yarn run typeorm --dataSource=src/data-source.ts migration:revert",
    "schema:drop": "yarn run typeorm --dataSource=src/data-source.ts schema:drop",
    "seed:run": "yarn typeorm-extension seed:run",
    "seed:create": "yarn typeorm-extension seed:create",
    "watch:dev": "ts-node-dev src/main.ts",
    "debug:dev": "cross-env TS_NODE_CACHE=false ts-node-dev --inspect --ignore '/^src/.*\\.spec\\.ts$/' src/main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "NODE_ENV=test jest",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:cov": "NODE_ENV=test jest --coverage",
    "test:debug": "NODE_ENV=test node --inspect-brk -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json",
    "docs:dev": "vuepress dev -p 7070",
    "docs:build": "DEPLOY_ENV=gh-pages vuepress build",
    "docs:deploy": "yarn docs:build && gh-pages -d .vuepress/dist",
    "prepare": "husky",
    "release": "release-it"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.817.0",
    "@keyv/redis": "^4.4.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.1.2",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/cqrs": "^11.0.3",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/microservices": "^11.1.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.1.2",
    "@nestjs/swagger": "^11.2.0",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/throttler": "^6.4.0",
    "@nestjs/typeorm": "^11.0.0",
    "bcrypt": "^5.1.1",
    "cache-manager": "^6.4.3",
    "class-transformer": "~0.5.1",
    "class-validator": "^0.14.2",
    "compression": "^1.8.0",
    "express": "^5.1.0",
    "helmet": "^8.1.0",
    "jsonwebtoken": "^9.0.2",
    "keyv": "^5.3.3",
    "lodash": "^4.17.21",
    "mime-types": "^2.1.35",
    "morgan": "^1.10.0",
    "nestjs-cls": "^5.4.3",
    "parse-duration": "^1.1.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.16.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "source-map-support": "^0.5.21",
    "swagger-ui-express": "^5.0.1",
    "tslib": "^2.8.1",
    "typeorm": "^0.3.24",
    "typeorm-extension": "^3.7.1",
    "typeorm-transactional": "~0.5.0",
    "uuid": "^9.0.1"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": [
        "ts-jest",
        {
          "experimentalDecorators": true,
          "emitDecoratorMetadata": true
        }
      ]
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "<rootDir>/boilerplate.polyfill.ts"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-angular": "^19.8.1",
    "@faker-js/faker": "^8.0.0",
    "@handfish/hygen": "^6.1.6",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/schematics": "^11.0.5",
    "@nestjs/testing": "^11.1.2",
    "@types/bcrypt": "^5.0.2",
    "@types/compression": "^1.8.0",
    "@types/express": "^5.0.2",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/lodash": "^4.17.17",
    "@types/mime-types": "^2.1.4",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/passport-jwt": "^3.0.13",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "@vuepress/client": "^2.0.0-rc.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-n": "^16.6.1",
    "eslint-plugin-prettier": "^5.4.0",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-sonarjs": "^3.0.2",
    "eslint-plugin-unicorn": "^59.0.1",
    "gh-pages": "^6.1.1",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "lint-staged": "~15.2.0",
    "nestjs-seeder": "^0.3.2",
    "prettier": "^3.5.3",
    "release-it": "^17.0.1",
    "supertest": "^7.1.1",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.8.3",
    "vue": "^3.5.14"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
